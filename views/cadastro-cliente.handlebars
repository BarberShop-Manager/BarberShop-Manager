<div class="cadastro_cliente">
    <form action="/cadastro/users/new-user" method="POST">
        <div>
            <label for="nome">Nome:</label>
            <input type="text" id="nome" name="_name">
        </div>
        <div>
            <label for="nome">Nome de Usuario:</label>
            <input type="text" id="_username" name="_username">
        </div>
        <div>
            <label for="email">Email:</label>
            <input type="email" id="email" name="_email">
        </div>
        <div>
            <label for="cpf">CPF:</label>
            <input type="text" maxlength="11" minlength="" id="cpf" name="_cpf">
        </div>
        <div>
            <label for="birthDay">Data de Nascimento:</label>
            <input type="date" id="birthDay" name="_date" value="2000-01-01">
        </div>
        <div>
            <label for="telephone">Telefone:</label>
            <input type="text" id="telephone" name="_telephone" >
        </div>
        <div>
            <label for="senha">Senha:</label>
            <input type="password" id="senha" name="_password">
        </div>
        <div>
            <label for="confirmarSenha">Confimar Senha:</label>
            <input type="password" id="confirmarSenha" name="confimarSenha"> <br>
        </div>
        <div id="botao_cadastro">
            <button type="submit">Cadastrar</button>
        </div>
        <div>
            <input type="hidden" id="nivel" name="_nivel" value="2"> <br>
        </div>
    </form>
</div>